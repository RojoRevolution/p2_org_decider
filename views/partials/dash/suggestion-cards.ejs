<% for(let i=0; i < ideaName.length; i++) { %>
    <div class="card m-2 card-adjust">
        <div class="card-body">
            <h5 class="card-title">
                <%= ideaName[i]%>
            </h5>
            <p>
                <%= ideaDescription[i]%>
            </p>
            <btn class="btn btn-secondary btn-sm" data-attribute="<%= ideaIds[i]%>">Put Up For Vote</btn>
        </div>
    </div>
    <% }; %>