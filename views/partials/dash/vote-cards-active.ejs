<% for(let i=0; i < ideaNameTrue.length; i++) { %>
    <div class="card m-2 card-adjust">
        <div class="card-body">
            <div class="row">
                <div class="col-2">
                    <a hfef="#"><i class=" fas fa-arrow-alt-circle-up icon-size"></i></a>
                    <p id="activeVote" class="voteCount">0</p>
                    <a hfef="#"><i class=" my-2 fas fa-arrow-alt-circle-down icon-size"></i></a>
                </div>
                <div class="col-10">
                    <h5 class="card-title">
                        <%= ideaNameTrue[i]%>
                    </h5>
                    <p class="card-text">
                        <%= ideaTextTrue[i]%>
                    </p>
                </div>
            </div>
        </div>
    </div>
    <% } %>