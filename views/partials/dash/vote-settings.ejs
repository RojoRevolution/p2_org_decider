<!-- Partial holds the Vote Setting (timer) options. No Dynamic Content in this partial -->
<div>
    <div class="d-flex justify-content-between">
        <div>
            <h4>How Long Will The Vote Be Active For?</h4>
        </div>
    </div>
    <div class="my-3">
        <form id="voteSettingsForm">
            <div class="form-check mb-3 dis-inline">
                <input class="form-check-input" type="radio" name="time" id="flexRadioDefault1" value="1" checked>
                <label class="form-check-label" for="flexRadioDefault1">
                    1 Minute
                </label>
            </div>
            <div class="form-check dis-inline ">
                <input class="form-check-input" type="radio" name="time" id="flexRadioDefault1" value="5">
                <label class="form-check-label" for="flexRadioDefault1">
                    5 Minute
                </label>
            </div>
            <div class="form-check dis-inline ">
                <input class="form-check-input" type="radio" name="time" id="flexRadioDefault1" value="10">
                <label class="form-check-label" for="flexRadioDefault1">
                    10 Minute
                </label>
            </div>
            <div>
                <button id="cancelVote" type="button" class="btn btn-secondary mb-3 dis-inline">Cancel</button>
                <button type="submit" class="btn btn mb-3 text-light dis-inline bg-color">Start Vote</button>
            </div>
    </div>
    </form>
</div>